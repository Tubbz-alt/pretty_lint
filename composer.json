{
  "name": "pnnl/pretty_lint",
  "description": "Pretty Lint JSON and YAML files",
  "authors": [
    {
      "name": "Thom Williams",
      "email": "thomscode@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Pnnl\\PrettyJSONYAML\\": "src"
    }
  },
  "config": {
    "sort-packages": true
  },
  "license": "MIT",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": ">=5.6",
    "dflydev/dot-access-data": "^1.0",
    "phpro/grumphp": "^0.14"
  },
  "require-dev": {
    "jakub-onderka/php-parallel-lint": "^1.0",
    "phpmd/phpmd": "^2.6",
    "squizlabs/php_codesniffer": "^3.2"
  },
  "scripts": {
    "cbf": "phpcbf",
    "grumphp": "grumphp --ansi"
  },
  "type": "project"
}